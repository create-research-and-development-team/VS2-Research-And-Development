{
  "schemaVersion": 1,
  "id": "vs_rnd",
  "version": "${version}",
  "name": "VS Research and Development",
  "description": "An Addon Template for VS2",
  "authors": [
    "PriestOfFern"
  ],
  "contact": {
    "homepage": "https://valkyrienskies.org/",
    "sources": "https://github.com/ValkyrienSkies/Eureka"
  },
  "license": "Apache-2.0",
  "icon": "assets/vs_addon_template/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.valkyrienskies.vs_rnd.fabric.VSRndModFabric"
    ],
    "client": [
      "org.valkyrienskies.vs_rnd.fabric.VSRndModFabric${'$'}Client"
    ],
    "preLaunch": [
      "org.valkyrienskies.vs_rnd.fabric.ValkyrienPreLaunch"
    ]
  },
  "mixins": [
    "vs_rnd-common.mixins.json",
    "vs_rnd.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.18.2",
    "valkyrienskies": ">=${vs2_version}",
    "fabricloader": ">=0.14.21"
  }
}